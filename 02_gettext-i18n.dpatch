#! /bin/sh /usr/share/dpatch/dpatch-run
## 02_gettext-i18n.dpatch by Tobias Grimm <tg@e-tobi.net>
##
## All lines beginning with `## DP:' are a description of the patch.
## DP: Adds gettext i18n.

@DPATCH@
diff -urNad vdr-plugin-wapd-0.9~/Makefile vdr-plugin-wapd-0.9/Makefile
--- vdr-plugin-wapd-0.9~/Makefile	2008-02-12 00:53:25.000000000 +0100
+++ vdr-plugin-wapd-0.9/Makefile	2008-02-12 00:53:53.000000000 +0100
@@ -53,7 +53,7 @@
 
 ### The object files (add further files here):
 
-OBJS = $(PLUGIN).o server.o tools.o access.o i18n.o
+OBJS = $(PLUGIN).o server.o tools.o access.o
 
 LIBS = -lcrypt
 
@@ -71,9 +71,34 @@
 
 -include $(DEPFILE)
 
+### Internationalization (I18N):
+
+PODIR     = po
+LOCALEDIR = $(VDRDIR)/locale
+I18Npo    = $(wildcard $(PODIR)/*.po)
+I18Nmsgs  = $(addprefix $(LOCALEDIR)/, $(addsuffix /LC_MESSAGES/vdr-$(PLUGIN).mo, $(notdir $(foreach file, $(I18Npo), $(basename $(file))))))
+I18Npot   = $(PODIR)/$(PLUGIN).pot
+
+%.mo: %.po
+	msgfmt -c -o $@ $<
+
+$(I18Npot): $(wildcard *.c)
+	xgettext -C -cTRANSLATORS --no-wrap --no-location -k -kwmltr -ktr -ktrNOOP --msgid-bugs-address='<tg@e-tobi.net>' -o $@ $^
+
+%.po: $(I18Npot)
+	msgmerge -U --no-wrap --no-location --backup=none -q $@ $<
+	@touch $@
+
+$(I18Nmsgs): $(LOCALEDIR)/%/LC_MESSAGES/vdr-$(PLUGIN).mo: $(PODIR)/%.mo
+	@mkdir -p $(dir $@)
+	cp $< $@
+
+.PHONY: i18n
+i18n: $(I18Nmsgs)
+
 ### Targets:
 
-all: libvdr-$(PLUGIN).so wappasswd
+all: libvdr-$(PLUGIN).so wappasswd i18n
 
 libvdr-$(PLUGIN).so: $(OBJS)
 	$(CXX) $(CXXFLAGS) -shared $(OBJS) $(LIBS) -o $@
@@ -91,4 +116,5 @@
 	@echo Distribution package created as $(PACKAGE).tgz
 
 clean:
+	@-rm -f $(PODIR)/*.mo $(PODIR)/*.pot
 	@-rm -f $(OBJS) $(DEPFILE) *.so *.tgz core* *~ wappasswd
